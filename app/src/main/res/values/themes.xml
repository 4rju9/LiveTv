<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LiveTV" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="ThemePrimary">@color/deep_sky_blue</item>
        <item name="colorPrimary">@color/lightBlue</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/lightBlue</item>
        <!-- Customize your theme here. -->
    </style>

    <attr name="ThemePrimary" format="color" />
    <attr name="ThemeSecondary" format="color" />
    <attr name="ThemeTextColor" format="color" />
    <attr name="ThemeIconColor" format="color" />
    <attr name="ThemeIconColorSecondary" format="color" />
    <attr name="ThemeIconInactiveColor" format="color" />

    <!-- Custom Theme Deep Sky Blue -->
    <style name="DeepSkyBlueNav" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="ThemePrimary">@color/deep_sky_blue</item>
        <item name="ThemeSecondary">@color/white</item>
        <item name="ThemeTextColor">@color/white</item>
        <item name="ThemeIconColor">?attr/ThemeSecondary</item>
        <item name="ThemeIconColorSecondary">?attr/ThemePrimary</item>
        <item name="ThemeIconInactiveColor">?attr/ThemeTextColor</item>
        <item name="colorPrimary">?attr/ThemePrimary</item>
        <item name="android:textColorPrimary">?attr/ThemeTextColor</item>
        <item name="colorPrimaryVariant">?attr/ThemePrimary</item>
        <item name="android:statusBarColor">#00A9F7</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

